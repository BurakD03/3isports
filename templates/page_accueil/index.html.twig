            {% endif %}
                <div class="item-wrap">
                    <div class="img-scale-animate mb-2" id="grid-image-1"> 
                      <a class="img-overlay-70" href="/article/{{grid.article.id}}"> 
                        <img width="670" height="324" src="{{ asset('uploads/images/articles/' ~ grid.article.thumbnail.image)}}" class="img-fluid mb-10 width-100 wp-post-image" alt="{{grid.article.alt}}" loading="lazy"> 
                      </a>
                      <div class="mask-content-lg">
                        <div class="topic-box-bottom-lg">
                          <a href="/categorie/{{grid.article.sousCategorie.categorie.id}}">
