<!-- Initialize Swiper -->
<script>
  var swiper = new Swiper(".mySwiper", {
    slidesPerView: 3,
    spaceBetween: 30,
    freeMode: true,
    breakpoints: {
      "@0.00": {
        slidesPerView: 1.2,
        spaceBetween: 10,
      },
      "@0.75": {
        slidesPerView: 2,
        spaceBetween: 20,
      },
      "@1.00": {
        slidesPerView: 3,
        spaceBetween: 10,
      },
      "@1.50": {
        slidesPerView: 4,
        spaceBetween: 10,
      },
    },
  });








// WEEKLY TOP NEWS
